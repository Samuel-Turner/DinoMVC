@{
    ViewData["Title"] = "Dino";
}
<br />
<div class="container row p-0 m-0">
    <div class="col-8">
        <h2 class="test-info">Dinosaurs</h2>
    </div>
    <div class="col-4">
        <a asp-action="Upsert" asp-controller="Dino" class="btn btn-primary form-control text-white"> Add New Dino</a>
    </div>
    <div class="col-12 border p-3">
        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Period</th>
                    <th>Diet</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>
@section Scripts{
    <script src="~/js/dinosList.js"></script>
}