@model DinoMVC.Models.Dino

<br />
<h2 class="text-info" style="border-bottom: 1px solid #000;">@(Model.Name) Dino</h2>
<br />

<div class="border container" style="padding:40px;">
    <div class="form-group row">
        <div class="col-4">
            <label>Dino Id :</label>
        </div>
        <div class="col-8">
            <label>@Model.Id</label>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-4">
            <label>Time Period :</label>
        </div>
        <div class="col-8">
            <label>@Model.Period</label>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-4">
            <label>Diet :</label>
        </div>
        <div class="col-8">
            <label>@Model.Diet</label>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-5">
            <a asp-action="Index" class="btn btn-success form-control"> Back to List </a>
        </div>
    </div>
</div>
